{
  "name": "marosca-web",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/createEnvirontmentFile.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:githubio": "npm run config -- --environment=prod && ng build marosca-web --configuration production --output-path docs && cp ./copy_to_docs/CNAME ./docs/CNAME && cp ./docs/index.html ./docs/404.html",
    "build:client": "npm run config -- --environment=prod && ng build marosca-web --configuration production && cp ./copy_to_docs/CNAME ./dist/marosca-web/browser/CNAME && cp ./dist/marosca-web/browser/index.html ./dist/marosca-web/browser/404.html",
    "build:ssr": "npm run build:client && ng run marosca-web:server:production",
    "serve:ssr": "NODE_ENV=production node ./dist/marosca-web/server/main.js",
    "dev:ssr": "ng run marosca-web:serve-ssr",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "prerender": "ng run marosca-web:prerender",
    "postinstall": "npm run build:ssr",
    "heroku": "node ./dist/marosca-web/server/main.js && bash ./scripts/purgeCaches.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.5",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/forms": "~12.0.5",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/platform-server": "~12.0.5",
    "@angular/router": "~12.0.5",
    "@contentful/rich-text-html-renderer": "^14.1.2",
    "@contentful/rich-text-types": "^14.1.2",
    "@nguniversal/express-engine": "^12.0.2",
    "@popperjs/core": "^2.8.2",
    "bodymovin": "^4.13.0",
    "bootstrap": "^4.6.0",
    "contentful": "^8.3.7",
    "express": "^4.17.3",
    "jquery": "^3.5.1",
    "lottie-web": "^5.7.11",
    "redis": "^3.1.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@nguniversal/builders": "^12.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jquery": "^3.5.5",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "dotenv": "^10.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4",
    "yargs": "^17.0.1"
  }
}
